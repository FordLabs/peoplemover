{
  "name": "peoplemover",
  "version": "1.0.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "prebuild": "npm run generate-build-version",
    "start": "HTTPS=true react-scripts start",
    "start:e2e": "react-scripts start",
    "start:backend": "cd ../ && SPRING_PROFILES_ACTIVE=e2e-test ./gradlew api:bootRun",
    "test:unit": "react-scripts test",
    "test:unit:ci": "CI=true react-scripts test --runInBand",
    "test:unit:coverage": "CI=true react-scripts test --coverage --testResultsProcessor jest-sonar-reporter",
    "cy:setup": "BROWSER=none start-server-and-test start:backend 8080 start:e2e 3000",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "npm run cy:setup cy:open",
    "test:e2e:ci": "npm run cy:setup cy:run",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit:ci && npm run test:e2e:ci",
    "coverage": "npm run test:unit:coverage",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint --fix src/**/*.ts src/**/*.tsx",
    "analyze": "node analyze.js",
    "tests:pre-commit": "CI=true react-scripts test --findRelatedTests",
    "prepare": "cd .. && husky install"
  },
  "lint-staged": {
    "**/*.+(spec.tsx|test.tsx)": [
      "npm run tests:pre-commit"
    ],
    "**/*.+(js|ts|tsx|spec.tsx|test.tsx)": [
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "@types/react-beautiful-dnd": "^13.1.2",
    "axios": "^0.21.2",
    "env-cmd": "^8.0.2",
    "flagsmith": "^1.6.0",
    "focus-trap-react": "^8.4.1",
    "js-file-download": "^0.4.10",
    "json2csv": "^4.5.4",
    "jwt-decode": "^3.0.0-beta.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.28",
    "react": "^17.0.0",
    "react-a11y-dialog": "^6.1.5",
    "react-beautiful-dnd": "^13.1.0",
    "react-datepicker": "^2.14.1",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.3.0",
    "react-select": "^3.0.8",
    "react-switch": "^6.0.0",
    "react-text-mask": "^5.4.3",
    "recoil": "^0.7.2",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.2",
    "@babel/traverse": "^7.8.6",
    "@babel/types": "^7.8.7",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^26.0.24",
    "@types/jest-axe": "^3.5.1",
    "@types/json2csv": "^4.5.1",
    "@types/jwt-decode": "^2.2.1",
    "@types/moment-timezone": "^0.5.13",
    "@types/react": "^17.0.44",
    "@types/react-datepicker": "^2.11.0",
    "@types/react-dom": "^17.0.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-select": "^3.0.10",
    "@types/react-text-mask": "^5.4.6",
    "awesome-typescript-loader": "^5.2.1",
    "axe-core": "^4.1.1",
    "cypress": "^10.4.0",
    "cypress-axe": "^1.0.0",
    "eslint": "^8.15.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.0",
    "jest-axe": "^4.1.0",
    "jest-dom": "^4.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.4.1",
    "react-scripts": "5.0.0",
    "react-select-event": "^5.5.0",
    "sass": "^1.42.0",
    "source-map-loader": "^0.2.4",
    "start-server-and-test": "^1.11.1",
    "typescript": "^4.6.4"
  },
  "jest": {
    "resetMocks": false,
    "clearMocks": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "fsevents": "^2.1.3"
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
